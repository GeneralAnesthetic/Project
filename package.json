    "bcryptjs": "^2.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "joi": "^14.0.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  }
